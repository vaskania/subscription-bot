{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "error",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "operator-linebreak": "off",
    "linebreak-style": "off",
    "implicit-arrow-linebreak": "off",
    "indent": "off",
    "import/no-useless-path-segments": "off",
    "import/no-dynamic-require": "off",
    "global-require":"off",
    "object-curly-newline": "off"
  }
}
